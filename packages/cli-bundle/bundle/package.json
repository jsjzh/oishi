{
  "name": "jarvis-bundle-base",
  "version": "0.1.0",
  "description": "该项目用来打包 jarvis-bundle 的 base 包，为什么有这个？因为 react-native 的狗*路径处理机制",
  "license": "MIT",
  "author": "金哲豪 <jinzhehao@souche.com>",
  "private": true,
  "scripts": {
    "update": "yarn add -D @jarvis/cli-bundle",
    "bundle:ios": "jarvis-bundle bundle -p ios -e index.ios.base.js -o ./dist/index.ios.common.bundle -s ./dist/index.ios.common.bundle.map -c ./metro.config.js",
    "bundle:android": "jarvis-bundle bundle -p android -e index.android.base.js -o ./dist/index.android.common.bundle -s ./dist/index.android.common.bundle.map -c ./metro.config.js",
    "bundle:both": "yarn run bundle:ios && yarn run bundle:android"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@jarvis/cli-bundle": "^0.0.47",
    "metro-react-native-babel-preset": "^0.58.0"
  }
}
